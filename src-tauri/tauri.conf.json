{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "urban-dispatch",
	"version": "0.1.0",
	"identifier": "org.shtelo.urban-dispatch",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../build"
	},
	"app": {
		"windows": [
			{
				"title": "교통량 안정도 평가보고",
				"width": 1600,
				"height": 900
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
